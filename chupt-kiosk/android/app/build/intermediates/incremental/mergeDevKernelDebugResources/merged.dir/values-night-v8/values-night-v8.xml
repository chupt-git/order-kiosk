    <item name="nohoRadioColorOuterCircle">#B5BBC1</item>
    <item name="nohoRadioColorBackground">#000000</item>
    <item name="nohoRadioColorCenter">#2483C1</item>
  </style>
    <style name="Widget.Noho.Radio.Colors.Pressed" parent="None">
